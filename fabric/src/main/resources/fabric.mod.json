{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "${sources_url}",
        "sources": "${sources_url}",
        "issues": "${issues_url}"
    },
    "license": "${license}",
    "icon": "assets/${mod_id}/icon.png",
    "environment": "*",
    "entrypoints": {
        "client": [
            "com.raxdiam.toastbegone.ToastBegoneFabric"
        ],
        "modmenu": [
            "com.raxdiam.toastbegone.compat.ModMenuCompat"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json"
    ],
    "depends": {
        "fabricloader": "*",
        "minecraft": "~${minecraft_version}",
        "java": ">=${java_version}"
    },
    "suggests": {
        "modmenu": ">=11.0.0"
    }
}
  